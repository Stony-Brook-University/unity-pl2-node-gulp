div.unity-button, a.unity-button
{
width: 100%;
min-width: 200px;
height: 48px;
background-color: #cccccc;
float: left;
color: #000;

text-decoration: none;

-moz-border-radius: 6px;
  -webkit-border-radius: 6px;
  -o-border-radius: 6px;
  -ms-border-radius: 6px;
  -khtml-border-radius: 6px;
  border-radius: 6px;

  margin: 5px 0px;

  .wrapper
  {
    width: 100%;
    height: 100%;

    .text{

      padding: 7px 7px;
      height: 100%;
      font-family: Arial, sans serif;
      float: left;
      font-size: 0.8em;
      max-width: 230px;

      h2
      {
        font-size: 1.6em;
        margin: 0px;
        padding: 0px;
        border-bottom: 0px;
      }

    }

    .icon
    {
        width: 36px;
        height: 100%;
        background-color: #990000;
        float: right;
        position: relative;
        border-top-right-radius: 6px;
        border-bottom-right-radius: 6px; 

        img
        {
          width: 16px;
          height: 16px;
          position: absolute;
          top: 50%;
          left: 50%;
          margin: -8px 0 0 -8px;
        }
    }
  }

}

a.unity-button:hover
{
background-color: #ddd;
cursor: pointer;
}




